<?xml version="1.0" encoding="UTF-8"?>
<!-- Tests: basic formula types -->
<worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main">
    <dimension ref="A1:D3"/>
    <sheetData>
        <row r="1">
            <c r="A1"><v>10</v></c>
            <c r="B1"><v>20</v></c>
            <!-- Simple formula with cached result -->
            <c r="C1"><f>A1+B1</f><v>30</v></c>
            <!-- Formula referencing another sheet -->
            <c r="D1"><f>Sheet2!A1*2</f><v>100</v></c>
        </row>
        <row r="2">
            <!-- Array formula spanning multiple cells -->
            <c r="A2"><f t="array" ref="A2:B2">{1,2,3}</f><v>1</v></c>
            <c r="B2"><v>2</v></c>
            <!-- Shared formula (master) with text -->
            <c r="C2"><f t="shared" ref="C2:C3" si="0">A2+B2</f><v>3</v></c>
        </row>
        <row r="3">
            <c r="A3"><v>100</v></c>
            <c r="B3"><v>200</v></c>
            <!-- Shared formula copy (must include formula text for our parser) -->
            <c r="C3"><f t="shared" si="0">A3+B3</f><v>300</v></c>
        </row>
    </sheetData>
</worksheet>
